{
    "name": "tenshijs",
    "version": "1.0.0",
    "description": "",
    "main": "src/index.js",
    "scripts": {
        "dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
        "start": "ts-node -r tsconfig-paths/register src/index.ts",

        "test": "jest",

        "delete": "ts-node -r tsconfig-paths/register src/seeds/DeleteDBSeed.ts",
        "generalseed": "ts-node -r tsconfig-paths/register src/seeds/GeneralSeed.ts",

        "automation": "ts-node -r tsconfig-paths/register automation/Automation.ts",
        "autotest": "ts-node -r tsconfig-paths/register automation/AutomationTest.ts",

        "compile": "tsc",
        "copyjsonconfig": "copyfiles tenshi-config.json build",
        "copyjsonsrc": "copyfiles -u 1 src/data/json/*.json build/src",
        "copyjsontenshi": "copyfiles -u 1 tenshi/data/json/*.json build/tenshi",
        "copytemp": "copyfiles -u 1 src/templates/*.html build",
        
        "build": "npm run compile && npm run copyjsonsrc && npm run copyjsontenshi && npm run copyjsonconfig && npm run copytemp",
        "prod": "node -r module-alias/register build/src/index.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.13",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/mssql": "^8.1.1",
        "@types/multer": "^1.4.11",
        "@types/nodemailer": "^6.4.15",
        "@types/pg": "^8.6.6",
        "@types/supertest": "^6.0.2",
        "@types/uuid": "^10.0.0",
        "aws-sdk": "^2.1672.0",
        "bcrypt": "^5.1.1",
        "copyfiles": "^2.4.1",
        "cors": "^2.8.5",
        "express": "^4.19.2",
        "helmet": "^7.1.0",
        "jest": "^29.7.0",
        "jsonwebtoken": "^9.0.2",
        "mariadb": "^3.3.1",
        "module-alias": "^2.2.3",
        "mssql": "^10.0.1",
        "multer": "^1.4.5-lts.1",
        "mysql": "^2.18.1",
        "node-cache": "^5.1.2",
        "nodemailer": "^6.9.14",
        "pg": "^8.12.0",
        "rate-limiter-flexible": "^5.0.3",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "typeorm": "^0.3.20",
        "typescript": "^5.5.4",
        "class-transformer": "^0.5.1"
    },
    "_moduleAliases": {
        "tenshi": "build/tenshi",     
        "@TenshiJS": "build/tenshi",
        "@index": "build/src",
        "@modules": "build/src/modules",
        "@templates": "build/templates",
        "@entity": "build/src/entity",
        "@data": "build/data/json",
        "@utils": "build/src/utils"
    }
}
